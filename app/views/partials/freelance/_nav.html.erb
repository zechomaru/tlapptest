<aside class="main-sidebar">
<!-- sidebar: style can be found in sidebar.less -->
	<section class="sidebar">
	  <!-- Sidebar user panel (optional) -->
	  <div class="user-panel">
	    <div class="pull-left image">
	      <% if !current_freelance.avatar.exists? %>
	        <% if current_freelance.gender.capitalize == "Masculino" %>
	            <%= image_tag "avatar5.png", class: "img-circle", alt: "User Image" %>
	        <% else %>
	            <%= image_tag "avatar2.png", class: "img-circle", alt: "User Image" %>
	        <% end %>
	      <% else %>
	          <%= image_tag current_freelance.avatar.url(:cover), class: "img-circle", alt: "User Image", style: "height: 45px !important;" %>
	      <% end %>
	    </div>
	    <div class="pull-left info">
	      <p><%= current_freelance.name.capitalize %> <%= current_freelance.last_name.capitalize %></p>
	      <!-- Status -->
	      <a href="#"><i class="fa fa-circle text-success"></i> Online</a>
	    </div>
	  </div>
	  <!-- search form (Optional) -->
	  <!-- /.search form -->
	  <!-- Sidebar Menu -->
	  <ul class="sidebar-menu">
	    <!--li class="header">HEADER</li-->
	    <!-- Optionally, you can add icons to the links -->

	    <!-- nav 1 -->
	      <% if current_page?(freelance_dashboard_path) %>
	        <li class="treeview active">
	      <%else%>
	        <li class="treeview">
	      <%end%>
	      <%= link_to freelance_dashboard_path do %>
	        <i class="fa fa-circle-o"></i>
	        <span>Home</span>
	      <% end %>
	    </li>

	    <!-- nav 2 -->
	    
	            
	            


	    
	  </ul><!-- /.sidebar-menu -->
	</section>
<!-- /.sidebar -->
</aside>