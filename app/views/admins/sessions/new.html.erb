<body class="test">
    <div class="login-card">
      <%= image_tag "logo_header-01.svg", class: "profile-img-card" %>
      <%= form_for(resource, as: resource_name, url: session_path(resource_name), html: {class: "form-signin" }) do |f| %>
        <span class="reauth-email"> </span>
        <%= f.email_field :email, autofocus: true, class: "form-control", placeholder: "Email", id: "inputEmail" %>
        <%= f.password_field :password, autocomplete: "off", class: "form-control", placeholder: "ContraseÃ±a", id: "inputPassword" %>
        <div class="checkbox">
          <% if devise_mapping.rememberable? -%>
            <%= f.check_box :remember_me %>
            <%= f.label :remember_me %>
          <% end -%>
        </div>
        <%= f.submit "Entrar", class: "btn btn-primary btn-block btn-lg btn-signin" %>
      <% end %>
      <div style="margin-top: 20px;">
        <%= render "admins/shared/links" %>
      </div>
    </div>
</body>
